# Example environment variables for production deployment

# Database Configuration
DATABASE_URL=jdbc:postgresql://postgres:5432/stammdatenverwaltung
DATABASE_USERNAME=stammdaten_user
DATABASE_PASSWORD=secure_db_password

# Security Configuration (Keycloak)
KEYCLOAK_CLIENT_SECRET=your_secret_here
KEYCLOAK_ISSUER_URI=https://your-keycloak/realms/stammdatenverwaltung
KEYCLOAK_API_AUDIENCE=stammdatenverwaltung-api

# Server Configuration
SERVER_PORT=8080

# Spring Profile
SPRING_PROFILES_ACTIVE=prod
