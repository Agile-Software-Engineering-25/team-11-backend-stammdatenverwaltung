# Example environment variables for production deployment

# Database Configuration
DATABASE_URL=jdbc:postgresql://postgres:5432/stammdatenverwaltung
DATABASE_USERNAME=stammdaten_user
DATABASE_PASSWORD=secure_db_password

# Security Configuration (Keycloak)
KEYCLOAK_ISSUER_URI=https://keycloak.sau-portal.de/realms/sau
KEYCLOAK_JWK_SET_URI=https://keycloak.sau-portal.de/realms/sau/protocol/openid-connect/certs

# Server Configuration
SERVER_PORT=8080

# Spring Profile
SPRING_PROFILES_ACTIVE=prod
