meta {
  name: Get John (Admin)
  type: http
  seq: 2
}

get {
  url: http://localhost:8080/api/v1/user?also_get_name_and_email=true
  body: json
  auth: inherit
}

params:query {
  also_get_name_and_email: true
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "filter": [
      {
        "key": "firstname",
        "operator": "EQUALS",
        "value": "John"
      },
      {
        "key": "email",
        "operator": "CONTAINS",
        "value": "@example.com"
      }
    ],
    "role": "ADMIN"
  }
}

settings {
  encodeUrl: false
}
