meta {
  name: Students
  type: http
  seq: 4
}

get {
  url: http://localhost:8080/api/v1/user?also_get_name_and_email=true
  body: json
  auth: inherit
}

params:query {
  also_get_name_and_email: true
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "filter": [
      {
        "key": "userId",
        "operator": "GREATER_THAN",
        "value": 100
      },
      {
        "key": "role",
        "operator": "EQUALS",
        "value": "STUDENT"
      }
    ]
  }
}

settings {
  encodeUrl: false
}
